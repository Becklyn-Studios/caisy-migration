{
    "name": "@becklyn/caisy-migration",
    "version": "0.2.0",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 0",
        "lint": "eslint . --max-warnings 0 && prettier --check \"./**/*.{ts,tsx}\"",
        "format": "eslint . --fix && prettier --write \"./**/*.{ts,tsx,scss}\"",
        "build": "rm -rf ./dist && tsc -p tsconfig-es.json && tsc -p tsconfig-cjs.json",
        "prepublishOnly": "npm run lint && npm run build",
        "release": "npm run build && npm run lint && npm run test && changeset version && changeset publish"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Becklyn-Studios/caisy-migration.git"
    },
    "files": [
        "./dist"
    ],
    "bin": {
        "migrate": "dist/bin/migrate.js"
    },
    "dependencies": {
        "@caisy/sdk": "^1.3.3",
        "dotenv": "^17.3.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0"
    },
    "peerDependencies": {
        "@caisy/sdk": "^1.3.3"
    },
    "devDependencies": {
        "@becklyn/eslint": "^1.3.1",
        "@becklyn/prettier": "^2.1.9",
        "@becklyn/tsconfig": "^1.1.3",
        "@changesets/cli": "^2.29.8",
        "typescript": "^5.9.3"
    },
    "exports": {
        ".": {
            "types": "./dist/cjs/index.d.ts",
            "require": "./dist/cjs/index.js",
            "import": "./dist/es/index.js",
            "default": "./dist/cjs/index.js"
        }
    },
    "prettier": "@becklyn/prettier"
}
