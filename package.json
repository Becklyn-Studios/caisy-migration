{
    "name": "@becklyn/caisy-migration",
    "version": "0.1.0",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 0",
        "lint": "eslint . --max-warnings 0 && prettier --check \"./**/*.{ts,tsx}\"",
        "format": "eslint . --fix && prettier --write \"./**/*.{ts,tsx,scss}\"",
        "build": "npm run clean && swc . -d dist && tsc && chmod +x dist/bin/migrate.js",
        "clean": "rm -rf dist",
        "prepublishOnly": "npm run lint && npm run build",
        "release": "npm run build && npm run lint && npm run test && changeset version && changeset publish"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Becklyn-Studios/caisy-migration.git"
    },
    "files": [
        "dist/**/*"
    ],
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.ts",
    "bin": {
        "migrate": "dist/bin/migrate.js"
    },
    "dependencies": {
        "@caisy/sdk": "^1.3.3",
        "dotenv": "^17.3.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0"
    },
    "peerDependencies": {
        "@caisy/sdk": "^1.3.3"
    },
    "devDependencies": {
        "@becklyn/eslint": "^1.3.1",
        "@becklyn/prettier": "^2.1.9",
        "@becklyn/tsconfig": "^1.1.3",
        "@changesets/cli": "^2.29.8",
        "@swc/cli": "^0.8.0",
        "@swc/core": "^1.15.13"
    },
    "prettier": "@becklyn/prettier"
}
